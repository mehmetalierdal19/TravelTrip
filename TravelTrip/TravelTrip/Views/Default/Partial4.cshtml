@using TravelTrip.Models.Siniflar
@model List<Blog>
@{
    Layout = null;
}

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <a href="/Blog/BlogDetay/@x.ID"><img src="@x.ImageURL" alt="" /></a>
            </div>
            <div class="bot-desc">
                <p>@x.Metin.Substring(0, 75)... </p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5", "Default")
</div>